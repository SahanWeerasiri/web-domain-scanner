# AI API Key (Required for AI-powered endpoint discovery)
GEMINI_API_KEY=your_actual_gemini_api_key_here
ANTHROPIC_API_KEY=your_actual_anthropic_api_key_here
OPENAI_API_KEY=your_actual_openai_api_key_here

# Domain Enumeration API Keys (Optional but recommended)
# VirusTotal API Key - Get your free API key from: https://www.virustotal.com/gui/join-us
# Free tier: 500 requests/day, 4 requests/minute
VIRUSTOTAL_API_KEY=your_virustotal_api_key_here

# Shodan API Key (Optional) - Get from: https://account.shodan.io/register
# Free tier: 100 query credits/month
SHODAN_API_KEY=your_shodan_api_key_here

# Censys API Credentials (Optional - No longer free)
CENSYS_API_ID=your_censys_api_id_here
CENSYS_API_SECRET=your_censys_api_secret_here

# Scanner Configuration (Optional - defaults will be used if not set)
MAX_THREADS=15
RATE_LIMIT=5
REQUEST_TIMEOUT=10
SCAN_TIMEOUT=300

# Proxy Settings (Optional)
# HTTP_PROXY=http://proxy.example.com:8080
# HTTPS_PROXY=https://proxy.example.com:8080
# PROXY_USER=username
# PROXY_PASS=password

# Output Settings (Optional)
OUTPUT_DIR=scan_results
LOG_LEVEL=INFO

# SSL Verification (Set to false for self-signed certificates)
VERIFY_SSL=true